{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home",
			"style": {
				"navigationBarTitleText": "402房间预约"
			}
		},
		{
			"path": "pages/messages",
			"style": {
				"navigationBarTitleText": "消息"
			}
		},
		{
			"path": "pages/mine",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/logIn",
			"style": {
				"navigationBarTitleText": "402房间预约"
			}
		},
		{
			"path": "pages/choose",
			"style": {
				"navigationBarTitleText": "402房间预约"
			}
		},
		{
			"path": "pages/completeInfor",
			"style": {
				"navigationBarTitleText": "完善个人信息"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"selectedColor": "#c8eeff",
		"list": [
			{
				"pagePath": "pages/home",
				"iconPath": "static/home.png",
				"selectedIconPath": "static/home_s.png"
			},
			{
				"pagePath": "pages/messages",
				"iconPath": "static/messages.png",
				"selectedIconPath": "static/messages_s.png"
			},
			{
				"pagePath": "pages/mine",
				"iconPath": "static/mine.png",
				"selectedIconPath": "static/mine_s.png"
			}
		]
	}
